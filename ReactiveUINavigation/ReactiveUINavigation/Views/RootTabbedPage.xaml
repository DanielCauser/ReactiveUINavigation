<?xml version="1.0" encoding="UTF-8" ?>
<rxui:ReactiveTabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="ReactiveUINavigation.Views.RootTabbedPage"
    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    xmlns:vm="clr-namespace:ReactiveUINavigation.ViewModels"
    xmlns:view="clr-namespace:ReactiveUINavigation.Views"
    x:TypeArguments="vm:MainTabbedViewModel"
    NavigationPage.HasNavigationBar="False">

    <NavigationPage Title="First">
        <x:Arguments>
            <view:FirstPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Second">
        <x:Arguments>
            <view:SecondPage />
        </x:Arguments>
    </NavigationPage>

</rxui:ReactiveTabbedPage>

